{
    "manifest_version":3,
    "name":"Get Quick Summary",
    "version":"1.0",

    "permissions": ["scripting","activeTab","storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon":"star_9274180.png"
    },
    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "background":{
      "service_worker":"background.js"
    },

    "options_page":"options.html",
    "host_permissions":["<all_urls>"]
}
